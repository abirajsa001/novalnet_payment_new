{
  "name": "payment-integration-template",
  "version": "4.1.0",
  "description": "Payment provider integration template",
  "main": "dist/main.js",
  "scripts": {
    "start": "node dist/main.js",
    "start:dev": "node_modules/.bin/nodemon -q dist/main.js",
    "lint": "prettier --check \"**/**/*.{ts,js,json}\" && eslint src test",
    "lint:fix": "prettier --write \"**/**/*.{ts,js,json}\" && eslint --fix src test",
    "build": "rm -rf dist && tsc",
    "dev": "ts-node src/main.ts",
    "watch": "nodemon --watch \"src/**\" --ext \"ts,json\" --ignore \"src/**/*.spec.ts\" --exec \"ts-node src/main.ts\"",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watch --detectOpenHandles",
    "connector:post-deploy": "node dist/connectors/post-deploy.js",
    "connector:pre-undeploy": "node dist/connectors/pre-undeploy.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@commercetools-backend/loggers": "23.2.3",
    "@commercetools/connect-payments-sdk": "0.27.1",
    "@commercetools/sdk-client-v2": "^2.5.0",
    "@fastify/autoload": "6.3.1",
    "@fastify/cors": "11.2.0",
    "@fastify/formbody": "8.0.2",
    "@fastify/http-proxy": "11.4.1",
    "@fastify/request-context": "6.2.1",
    "@fastify/static": "8.1.1",
    "@fastify/type-provider-typebox": "5.1.0",
    "@sinclair/typebox": "0.34.45",
    "dotenv": "16.5.0",
    "fastify": "5.6.2",
    "fastify-plugin": "5.1.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.9",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@jest/globals": "29.7.0",
    "@types/jest": "^29.5.14",
    "@types/node": "22.15.3",
    "@typescript-eslint/eslint-plugin": "8.51.0",
    "@typescript-eslint/parser": "8.51.0",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-jest": "28.11.0",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-unused-imports": "4.3.0",
    "globals": "^16.5.0",
    "jest": "^29.7.0",
    "msw": "2.7.6",
    "node-fetch": "3.3.2",
    "nodemon": "3.1.11",
    "prettier": "3.7.4",
    "ts-jest": "^29.4.6",
    "ts-node": "10.9.2",
    "typescript": "5.9.3"
  },
  "overrides": {
    "path-to-regexp": "3.3.0"
  }
}
